{
  "name": "@specify-bdd/plugin-cli",
  "version": "0.1.4",
  "description": "A plugin for Specify BDD adding CLI testing support.",
  "type": "module",
  "contributors": [
    "Adam Lacoste",
    "Michael Brown",
    "Wild Love"
  ],
  "license": "MIT",
  "main": "dist/main.js",
