{
    "entryPoints": [ "src/lib" ],
    "entryPointStrategy": "expand",
    "outputs": [
        {
            "name": "html",
            "path": "docs/html/typedoc"
        },
        {
            "name": "markdown",
            "path": "docs/markdown/typedoc"
        }
    ],
    "readme": "README.md",
    "exclude": [
        "**/*.test.ts", "**/__mocks__/**"
    ],
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": true,
    "githubPages": false,
    "plugin": [ "typedoc-plugin-markdown" ],
    "tsconfig": "tsconfig.build.json"
}
