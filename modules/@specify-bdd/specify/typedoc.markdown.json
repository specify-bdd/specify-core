{
    "entryPoints": [ "src/lib" ],
    "entryPointStrategy": "expand",
    "out": "docs/markdown/typedoc",
    "readme": "README.md",
    "exclude": [
        "**/*.test.ts", "**/__mocks__/**"
    ],
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": true,
    "plugin": [ "typedoc-plugin-markdown" ]
}